{
    "openapi": "3.0.0",
    "info": {
        "title": "Leasy Api Documentation",
        "version": "1.0.0"
    },
    "paths": {
        "/api/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Create a new user",
                "description": "Create a new user with the provided information",
                "operationId": "8a56853624e025573120a09a4c75d468",
                "requestBody": {
                    "description": "User data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "full_name",
                                    "phone_number",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "ofirgoldofir@gmail.com"
                                    },
                                    "full_name": {
                                        "type": "string",
                                        "example": "Ofir Goldberg"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "example": "0527576444"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "Aa123!@#"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User created successfully"
                    },
                    "500": {
                        "description": "An error occurred"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Login user",
                "description": "Authenticate and login user based on personal number",
                "operationId": "login",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "ofirgoldofir@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "Aa123!@#"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User logged in successfully"
                    },
                    "404": {
                        "description": "User not found"
                    },
                    "500": {
                        "description": "An error occurred while user logged in"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/chat/send-message/{uuid}": {
            "post": {
                "tags": [
                    "Chat"
                ],
                "summary": "Send a message to a chat",
                "description": "Sends a message to a specific chat identified by its UUID.",
                "operationId": "db372e1f498ed3dc425bbe33951ba399",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "The UUID of the chat",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "message": {
                                        "type": "string",
                                        "example": "Hello, how are you?"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Message sent successfully"
                    },
                    "404": {
                        "description": "Chat not found"
                    },
                    "500": {
                        "description": "An error occurred"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/lecture": {
            "get": {
                "tags": [
                    "Lectures"
                ],
                "description": "Retrieve lecture records. Supports pagination and sorting.",
                "operationId": "getLectures",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number for pagination.",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 2
                        }
                    },
                    {
                        "name": "sort_by",
                        "in": "query",
                        "description": "Sort by column (e.g., 'date', 'name').",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "date",
                                "name"
                            ]
                        }
                    },
                    {
                        "name": "sort_direction",
                        "in": "query",
                        "description": "Sort direction (e.g., 'asc', 'desc').",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "asc",
                                "desc"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response with lectures data"
                    },
                    "204": {
                        "description": "No content"
                    },
                    "500": {
                        "description": "An error occurred"
                    }
                }
            },
            "post": {
                "tags": [
                    "Lectures"
                ],
                "description": "Store a lecture with the provided details",
                "operationId": "postLecture",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "video"
                                ],
                                "properties": {
                                    "video": {
                                        "description": "The video file to upload",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "lecture created successfully"
                    },
                    "500": {
                        "description": "An error occurred"
                    }
                }
            }
        },
        "/api/lecture/{uuid}": {
            "get": {
                "tags": [
                    "Lectures"
                ],
                "description": "Fetch detailed information about a specific lecture using its UUID.",
                "operationId": "getLecture",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "The UUID of the lecture",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response with lecture data"
                    },
                    "404": {
                        "description": "Lecture not found"
                    },
                    "500": {
                        "description": "An error occurred"
                    }
                }
            }
        },
        "/api/quiz/questions/{uuid}": {
            "get": {
                "tags": [
                    "Quiz"
                ],
                "summary": "Get the questions of the quiz",
                "description": "Fetches all questions of a quiz.",
                "operationId": "0365c35651f8003e815d93e90099a89a",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "The UUID of the quiz",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Questions retrieved successfully"
                    },
                    "404": {
                        "description": "Quiz not found"
                    },
                    "204": {
                        "description": "No content"
                    },
                    "500": {
                        "description": "An error occurred"
                    }
                }
            }
        },
        "/api/quiz/answer/{uuid}": {
            "put": {
                "tags": [
                    "Quiz"
                ],
                "summary": "Answer quiz",
                "description": "Submits answers for a quiz",
                "operationId": "38686818dfd03ee9489dae4c0bd15c43",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "The UUID of the quiz",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Answers for the quiz questions",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "answers": {
                                        "description": "List of answers",
                                        "type": "array",
                                        "items": {
                                            "required": [
                                                "question_uuid",
                                                "answer"
                                            ],
                                            "properties": {
                                                "question_uuid": {
                                                    "description": "The UUID of the question",
                                                    "type": "string",
                                                    "format": "uuid"
                                                },
                                                "answer": {
                                                    "type": "string",
                                                    "example": "2"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Answers submitted successfully"
                    },
                    "400": {
                        "description": "This question cannot be answered"
                    },
                    "404": {
                        "description": "Some of the quiz properties not found"
                    },
                    "500": {
                        "description": "An error occurred"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get authenticated user",
                "description": "Returns the authenticated user's details",
                "operationId": "getAuthUser",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "404": {
                        "description": "User not found"
                    },
                    "500": {
                        "description": "An error occurred"
                    }
                }
            }
        },
        "/api/video/last-watched-time/{uuid}": {
            "put": {
                "tags": [
                    "Videos"
                ],
                "summary": "Update the last watched time for a video",
                "description": "Updates the last watched time for a specific video using the provided video UUID.",
                "operationId": "334262d4225b3be95a8471048d7378cb",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "The UUID of the video to update the last watched time",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "The last watched time to update",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "last_watched_time": {
                                        "type": "integer",
                                        "example": 120
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Last watched time updated successfully"
                    },
                    "404": {
                        "description": "Video not found"
                    },
                    "500": {
                        "description": "Internal server error"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/video/fix-audio/{uuid}": {
            "put": {
                "tags": [
                    "Videos"
                ],
                "summary": "Fix audio of a video",
                "description": "Fixes the audio issues of the specified video.",
                "operationId": "90e7b09d4d3df597c76fa28772796e26",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "The UUID of the video to fix audio",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Audio fixed successfully"
                    },
                    "404": {
                        "description": "Video not found"
                    },
                    "500": {
                        "description": "An error occurred"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        }
    }
}