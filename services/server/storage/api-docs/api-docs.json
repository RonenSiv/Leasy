{
    "openapi": "3.0.0",
    "info": {
        "title": "Leasy Api Documentation",
        "version": "1.0.0"
    },
    "paths": {
        "/api/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Create a new user",
                "description": "Create a new user with the provided information",
                "operationId": "8a56853624e025573120a09a4c75d468",
                "requestBody": {
                    "description": "User data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "full_name",
                                    "phone_number",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "ofirgoldofir@gmail.com"
                                    },
                                    "full_name": {
                                        "type": "string",
                                        "example": "Ofir Goldberg"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "example": "0527576444"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "Aa123!@#"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User created successfully"
                    },
                    "500": {
                        "description": "An error occurred while creating the user"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Login user",
                "description": "Authenticate and login user based on personal number",
                "operationId": "login",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "ofirgoldofir@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "Aa123!@#"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User logged in successfully"
                    },
                    "404": {
                        "description": "User not found"
                    },
                    "500": {
                        "description": "An error occurred while user logged in"
                    },
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get authenticated user",
                "description": "Returns the authenticated user's details",
                "operationId": "getAuthUser",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "404": {
                        "description": "User not found"
                    },
                    "500": {
                        "description": "An error occurred while fetching the user"
                    }
                }
            }
        },
        "/api/videos": {
            "post": {
                "tags": [
                    "Videos"
                ],
                "summary": "Store a video",
                "description": "Store a video with the provided name",
                "operationId": "store video",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "video"
                                ],
                                "properties": {
                                    "video": {
                                        "description": "The video file to upload",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "הסרטון עלה בהצלחה"
                    },
                    "500": {
                        "description": "אירעה שגיאה בעת העלאה"
                    },
                    "204": {
                        "description": "אין תוכן"
                    }
                }
            }
        }
    }
}